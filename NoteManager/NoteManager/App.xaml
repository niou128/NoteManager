             xmlns:app="clr-namespace:NoteManager"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <BooleanToVisibilityConverter x:Key="isVisible" />
        <app:RadioButtonConverter x:Key="radioButtonConverter" />
        <app:IsEnabledConverter x:Key="isEnableConverter" />
    </Application.Resources>
</Application>
